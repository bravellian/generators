        </Entry>
        <Entry DisplayName="Public Properties">
            <Entry.Match>
                <And>
                    <Or>
                        <Kind Is="Property" />
                        <Kind Is="Autoproperty" />
                    </Or>
                    <Access Is="Public" />
                </And>
            </Entry.Match>
            <Entry.SortBy>
                <Access />
                <Static />
                <Readonly />
                <Name />
            </Entry.SortBy>
        </Entry>
        <Entry DisplayName="Internal Properties">
            <Entry.Match>
                <And>
                    <Or>
                        <Kind Is="Property" />
                        <Kind Is="Autoproperty" />
                    </Or>
                    <Access Is="Internal" />
                </And>
            </Entry.Match>
            <Entry.SortBy>
                <Access />
                <Static />
                <Readonly />
                <Name />
            </Entry.SortBy>
        </Entry>
        <Entry DisplayName="ProtectedInternal Properties">
            <Entry.Match>
                <And>
                    <Or>
                        <Kind Is="Property" />
                        <Kind Is="Autoproperty" />
                    </Or>
                    <Access Is="ProtectedInternal" />
                </And>
            </Entry.Match>
            <Entry.SortBy>
                <Access />
                <Static />
