        <Entry DisplayName="Static Constructor">
            <Entry.Match>
                <And>
                    <Kind Is="Constructor" />
                    <Static />
                </And>
            </Entry.Match>
            <Entry.SortBy>
                <Readonly />
                <Name />
            </Entry.SortBy>
        </Entry>
        <Entry DisplayName="Public Constructors">
            <Entry.Match>
