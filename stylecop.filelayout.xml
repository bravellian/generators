                <And>
                    <Kind Is="Constant" />
                    <Access Is="PrivateProtected" />
                </And>
            </Entry.Match>
            <Entry.SortBy>
                <Access />
                <Static />
                <Readonly />
                <Name />
            </Entry.SortBy>
        </Entry>
        <Entry DisplayName="Private Constants">
            <Entry.Match>
                <And>
                    <Kind Is="Constant" />
                    <Access Is="Private" />
                </And>
            </Entry.Match>
            <Entry.SortBy>
                <Access />
                <Static />
                <Readonly />
                <Name />
            </Entry.SortBy>
        </Entry>
        <Entry DisplayName="Public Fields">
            <Entry.Match>
                <And>
                    <Kind Is="Field" />
                    <Access Is="Public" />
                </And>
            </Entry.Match>
            <Entry.SortBy>
