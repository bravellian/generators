                <Name />
            </Entry.SortBy>
        </Entry>
        <Entry DisplayName="Protected Enums">
            <Entry.Match>
                <And>
                    <Kind Is="Enum" />
                    <Access Is="Protected" />
                </And>
            </Entry.Match>
            <Entry.SortBy>
                <Access />
