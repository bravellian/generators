            </Entry.SortBy>
        </Entry>
        <Entry DisplayName="Indexers">
            <Entry.Match>
                <Kind Is="Indexer" />
            </Entry.Match>
            <Entry.SortBy>
                <Access />
                <Static />
                <Readonly />
            </Entry.SortBy>
        </Entry>
        <Entry DisplayName="Operators">
            <Entry.Match>
                <And>
                    <Kind Is="Operator" />
                </And>
            </Entry.Match>
            <Entry.SortBy>
                <Access />
                <Static />
                <Readonly />
                <Name />
            </Entry.SortBy>
        </Entry>
        <Entry DisplayName="Public Methods">
            <Entry.Match>
                <And>
                    <Kind Is="Method" />
                    <Access Is="Public" />
                </And>
            </Entry.Match>
            <Entry.SortBy>
                <Access />
