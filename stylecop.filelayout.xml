            </Entry.SortBy>
        </Entry>
        <Entry DisplayName="Protected Methods">
            <Entry.Match>
                <And>
                    <Kind Is="Method" />
                    <Access Is="Protected" />
                </And>
            </Entry.Match>
            <Entry.SortBy>
                <Access />
                <Static />
                <Readonly />
                <Name />
            </Entry.SortBy>
        </Entry>
        <Entry DisplayName="PrivateProtected Methods">
            <Entry.Match>
                <And>
                    <Kind Is="Method" />
                    <Access Is="PrivateProtected" />
                </And>
            </Entry.Match>
            <Entry.SortBy>
                <Access />
                <Static />
                <Readonly />
                <Name />
            </Entry.SortBy>
