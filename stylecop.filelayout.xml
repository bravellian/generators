                <And>
                    <Kind Is="Enum" />
                    <Access Is="Public" />
                </And>
            </Entry.Match>
            <Entry.SortBy>
                <Access />
                <Name />
            </Entry.SortBy>
        </Entry>
        <Entry DisplayName="Internal Enums">
            <Entry.Match>
                <And>
