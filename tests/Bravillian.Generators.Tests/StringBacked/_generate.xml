<Types xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../tooling/schemas/pw_generate.xsd">
    <StringBacked name="ErpRecordSource" namespace="Bravillian.Core" />
    
    <StringEnum name="AccountingSystemEnum" namespace="Bravillian.Core">
        <Value value="none" name="None" />
        <Value value="viewpoint_vista" name="ViewpointVista" display="Viewpoint Vista" />
        <Value value="viewpoint_spectrum" name="ViewpointSpectrum" display="Viewpoint Spectrum" />
        <Value value="cmic" name="Cmic" display="CMiC" />
        <Value value="acumatica" name="Acumatica" display="Acumatica" />
        <Value value="foundation" name="Foundation" display="Foundation" />
        <Value value="premier" name="Premier" display="Premier" />
        <Value value="coins" name="Coins" display="COINS" />
        <Value value="sage_intacct" name="SageIntacct" display="Sage Intacct" />
        <Value value="sage_100" name="Sage100" display="Sage 100" />
        <Value value="sage_300" name="Sage300" display="Sage 300" />
        <Value value="quickbooks" name="Quickbooks" display="Quickbooks" />
        <Value value="pwsql" name="PwSql" display="Bravillian Sql" />
    </StringEnum>

    <FastIdBacked name="BillingFrequencyIdentifier" namespace="Bravillian.Core"/>
    <GuidBacked name="AddressUsaStandardizedIdentifier" namespace="Bravillian.Core" default-format="false"/>
    <MultiValueString name="ErpConnectionString" namespace="Bravillian">
        <TypeProperty name="AccountingSystem" type="Bravillian.Core.AccountingSystemEnum" serializedName="erp" />
        <StringProperty name="ConnectionString" />
    </MultiValueString>
    <MultiValueString name="BasicPredicate" namespace="Bravillian.Core" separator="¦">
        <StringProperty name="Type" />
        <StringProperty name="KeyValue" />
    </MultiValueString>

    <NumberEnum name="VistaJobStatus" namespace="Bravillian" type="byte">
        <Value name="Pending" value="0"/>
        <Value name="Open" value="1"/>
        <Value name="SoftClose" display="Soft Close" value="2"/>
        <Value name="FinalClose" display="Final Close" value="3"/>
    </NumberEnum>
</Types>
